---
import { getTalks } from '../../../utils/i18n';

export async function getStaticPaths() {
  const talks = await getTalks('ja');

  return talks.map((talk) => ({
    params: { slug: talk.slug },
  }));
}

const { slug } = Astro.params;

return Astro.redirect(`/ja/garden/talks/${slug}`, 301);
